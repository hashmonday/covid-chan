{
  "kind": "collectionType",
  "collectionName": "foreign_workers",
  "info": {
    "name": "Foreign Workers"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "reference_number": {
      "type": "uid",
      "maxLength": 13,
      "minLength": 13,
      "required": true
    },
    "title": {
      "model": "titles"
    },
    "first_name": {
      "type": "string",
      "regex": "^[A-Z a-z]+$",
      "unique": false,
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true,
      "regex": "^[A-Z a-z]+$"
    }
  }
}
